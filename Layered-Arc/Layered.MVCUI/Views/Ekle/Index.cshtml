
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <div class="col-md-6" style="margin-left:auto;margin-right:auto;margin-top:10px">
            <form method="POST" action="~/Ekle/AddP">
                <div class="form-group">
                    <input type="text" name="Language" class="form-control" placeholder="Programlama Dilini Gir" />
                </div>
                <div class="form-group">
                    <input type="text" name="Founder" class="form-control" placeholder="Founder Gir" />
                </div>
                <div class="form-group">
                    <input type="text" name="Year" class="form-control" placeholder="Yıl" />
                </div>
                <div class="form-group">
                    <input type="text" name="IsPopular" class="form-control" placeholder="Popüler mi?" />
                </div>
                <div class="form-group">
                    <input type="submit" class="form-control" placeholder="Programlama Dilini Gir" />
                </div>
            </form>
        </div>
    </div>
</body>
</html>
